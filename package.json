{
  "name": "nest-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \\\\\\\"src/**/*.ts\\\\\\\" \\\\\\\"test/**/*.ts\\\\\\\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest-e2e.json",
    "build:contracts": "npm run build:typechain:registry && npm run build:typechain:resolver && npm run build:typechain:didregistry",
    "build:typechain:didregistry": "typechain --target ethers-v4 --outDir src/ethers 'abi/EthereumDIDRegistry.json'",
    "build:typechain:registry": "typechain --target ethers-v4 --outDir src/ethers './node_modules/@ensdomains/ens/build/contracts/ENSRegistry.json'",
    "build:typechain:resolver": "typechain --target ethers-v4 --outDir src/ethers './node_modules/@ensdomains/resolver/build/contracts/PublicResolver.json'"
  },
  "dependencies": {
    "@ensdomains/ens": "^0.4.5",
    "@ensdomains/resolver": "^0.2.4",
    "@ew-did-registry/did-ethr-resolver": "0.0.1-alpha.649.0",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/swagger": "^4.6.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "dgraph-js": "^20.3.0",
    "ethers": "^4.0.45",
    "grpc": "^1.24.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.4",
    "typechain": "^2.0.0",
    "web3": "^1.3.0"
  },
  "devDependencies": {
    "@typechain/ethers-v4": "^1.0.1",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.10",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.4.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
