{
  "name": "iam-cache-server",
  "version": "2.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "commit": "git-cz",
    "commit:retry": "git-cz --retry",
    "prebuild": "rimraf dist && npm run build:contracts",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "npm run build:contracts && nest start --debug 0.0.0.0:9229 --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "run-s migration:test && NODE_ENV=test jest -i \"/src/.*\\.spec\\.ts$\"",
    "test:e2e": "run-s migration:test && jest -i \"/test/.*\\.e2e.spec.(ts|js)$\" --force-exit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "build:contracts": "npm run build:typechain:registry && npm run build:typechain:resolver && npm run build:typechain:didregistry && npm run build:typechain:offerableIdentity && npm run build:typechain:identityManager && npm run build:typechain:domainNotifier && npm run build:typechain:stakingPool",
    "build:typechain:didregistry": "typechain --target ethers-v5 --out-dir src/ethers 'abi/EthereumDIDRegistry.json'",
    "build:typechain:registry": "typechain --target ethers-v5 --out-dir src/ethers './node_modules/@ensdomains/ens/build/contracts/ENSRegistry.json'",
    "build:typechain:resolver": "typechain --target ethers-v5 --out-dir src/ethers './node_modules/@ensdomains/resolver/build/contracts/PublicResolver.json'",
    "build:typechain:offerableIdentity": "typechain --target ethers-v5 --out-dir src/ethers './node_modules/@ew-did-registry/proxyidentity/build/contracts/OfferableIdentity.json'",
    "build:typechain:identityManager": "typechain --target ethers-v5 --out-dir src/ethers './node_modules/@ew-did-registry/proxyidentity/build/contracts/IdentityManager.json'",
    "build:typechain:domainNotifier": "typechain --target ethers-v5 --out-dir src/ethers './node_modules/@energyweb/iam-contracts/dist/contracts/DomainNotifier.json'",
    "build:typechain:stakingPool": "typechain --target ethers-v5 --out-dir src/ethers './node_modules/@energyweb/iam-contracts/dist/contracts/StakingPoolFactory.json'",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "docker:build": "docker-compose build app",
    "docker:start:app": "docker-compose up -d",
    "docker:start:resolver": "make start-universal-resolver",
    "docker:watch": "npm run docker:start:app && docker-compose logs -f app",
    "docker:down": "docker-compose down -v",
    "build:docs": "rimraf docs/api && typedoc --plugin typedoc-plugin-markdown --hideBreadcrumbs true ./src",
    "generate:jwtkeys": "openssl genrsa -out private.pem 2048 && openssl rsa -in private.pem -pubout > public.pem",
    "migrate:claim-subject": "node src/scripts/migrate_claim_subject.js",
    "migration:generate": "docker-compose run --rm app ./node_modules/.bin/typeorm migration:generate -n",
    "migration:run": "docker-compose run --rm app ./node_modules/.bin/typeorm migration:run && npm run migrate:claim-subject",
    "migration:revert": "docker-compose run --rm app ./node_modules/.bin/typeorm migration:revert",
    "migration:create": "docker-compose run --rm app ./node_modules/.bin/typeorm migration:create -n",
    "migration:test": "NODE_ENV=test ts-node ./node_modules/typeorm/cli.js migration:run --config test/config.ts",
    "release": "standard-version"
  },
  "dependencies": {
    "@energyweb/iam-contracts": "^3.2.0",
    "@ensdomains/ens": "^0.4.5",
    "@ensdomains/resolver": "^0.2.4",
    "@ethersproject/providers": "^5.4.5",
    "@ew-did-registry/did-document": "0.5.2-alpha.101.0",
    "@ew-did-registry/did-ethr-resolver": "0.5.2-alpha.101.0",
    "@ew-did-registry/did-ipfs-store": "0.5.2-alpha.101.0",
    "@ew-did-registry/jwt": "0.5.2-alpha.101.0",
    "@ew-did-registry/keys": "0.5.2-alpha.101.0",
    "@ew-did-registry/proxyidentity": "0.5.2-alpha.101.0",
    "@nestjs/axios": "0.0.2",
    "@nestjs/bull": "^0.4.1",
    "@nestjs/common": "^8.0.6",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.6",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/graphql": "^8.0.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/typeorm": "^8.0.2",
    "@sentry/integrations": "^6.2.2",
    "@sentry/node": "^6.2.2",
    "@typechain/ethers-v5": "^7.0.1",
    "@types/jwt-decode": "^2.2.1",
    "apollo-server-express": "^2.21.1",
    "bull": "^3.18.0",
    "chance": "^1.1.7",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cockatiel": "^2.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "dgraph-js": "^20.11.0",
    "dotenv": "^10.0.0",
    "ethers": "5.4.7",
    "graphql": "^15.5.1",
    "graphql-fields": "^2.0.3",
    "graphql-tools": "^7.0.4",
    "grpc": "^1.24.6",
    "helmet": "^4.4.1",
    "lodash.chunk": "^4.2.0",
    "nats": "^1.4.12",
    "passport-did-auth": "^1.0.0-alpha.23",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.3.0",
    "swagger-ui-express": "^4.1.4",
    "typechain": "^5.0.0",
    "typeorm": "^0.2.31",
    "useragent": "^2.3.0",
    "uuid": "^8.3.2",
    "wait-for-expect": "^3.0.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@nestjs/cli": "^7.5.6",
    "@nestjs/schematics": "^7.2.9",
    "@nestjs/testing": "^8.0.6",
    "@sentry/types": "^6.2.2",
    "@types/bull": "^3.14.4",
    "@types/chance": "^1.1.3",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.22",
    "@types/lodash.chunk": "^4.2.6",
    "@types/node": "^13.9.1",
    "@types/passport-jwt": "^3.0.3",
    "@types/redis": "^2.8.28",
    "@types/supertest": "^2.0.8",
    "@types/useragent": "^2.3.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "base64url": "^3.0.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^4.3.8",
    "jest": "27.0.6",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^11.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.1",
    "supertest": "^4.0.2",
    "ts-jest": "27.0.4",
    "ts-loader": "^6.2.1",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.22.0",
    "typedoc-plugin-markdown": "^3.11.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build:docs",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
