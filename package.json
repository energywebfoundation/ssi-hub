    "ethers": "^5.7.0",
    "graphql": "^15.5.1",
    "graphql-fields": "^2.0.3",
    "graphql-tools": "^8.0.0",
    "grpc": "^1.24.6",
    "helmet": "^4.4.1",
    "lodash.chunk": "^4.2.0",
    "ms": "^2.1.3",
    "multiformats": "^9.6.4",
    "nats": "^1.4.12",
    "pako": "^2.0.4",
    "parse-duration": "^1.0.2",
    "passport": "^0.6.0",
