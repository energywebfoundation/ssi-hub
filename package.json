{
  "name": "iam-cache-server",
  "version": "2.12.1",
  "description": "",
  "author": "",
  "engines": {
    "node": ">=18.0",
    "npm": ">=10.0"
  },
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "clean": "rimraf src/ethers/factories node_modules dist",
    "commit": "git-cz",
    "commit:retry": "git-cz --retry",
    "prebuild": "rimraf dist && npm run build:contracts",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test,e2e}/**/*.ts\" --fix",
    "test": "NODE_ENV=test jest -i \"/src/.*\\.spec\\.ts$\" --verbose --runInBand --force-exit",
    "test:e2e": "jest -i \"/e2e/.*\\.e2e.spec.(ts|js)$\" --force-exit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "build:contracts": "run-p build:typechain:*",
    "build:typechain:didregistry": "typechain --target ethers-v5 --out-dir src/ethers 'abi/EthereumDIDRegistry.json'",
    "build:typechain:registry": "typechain --target ethers-v5 --out-dir src/ethers './node_modules/@ensdomains/ens-contracts/artifacts/contracts/registry/ENSRegistry.sol/ENSRegistry.json'",
    "build:typechain:resolver": "typechain --target ethers-v5 --out-dir src/ethers './node_modules/@ensdomains/ens-contracts/artifacts/contracts/resolvers/PublicResolver.sol/PublicResolver.json'",
    "build:typechain:offerableIdentity": "typechain --target ethers-v5 --out-dir src/ethers './node_modules/@ew-did-registry/proxyidentity/build/contracts/OfferableIdentity.json'",
    "build:typechain:identityManager": "typechain --target ethers-v5 --out-dir src/ethers './node_modules/@ew-did-registry/proxyidentity/build/contracts/IdentityManager.json'",
    "build:typechain:domainNotifier": "typechain --target ethers-v5 --out-dir src/ethers './node_modules/@energyweb/credential-governance/dist/contracts/DomainNotifier.json'",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "docker:start:app": "docker-compose up -d",
    "docker:start:resolver": "make start-universal-resolver",
    "docker:watch": "npm run docker:start:app && docker-compose logs -f app",
    "docker:down": "docker-compose down -v",
    "build:docs": "rimraf docs/api && typedoc --plugin typedoc-plugin-markdown --hideBreadcrumbs true ./src",
    "generate:jwtkeys": "openssl genrsa -out private.pem 2048 && openssl rsa -in private.pem -pubout > public.pem",
    "migrate:claim-subject": "node src/scripts/migrate_claim_subject.js",
    "migrate:asset-data": "node src/scripts/migrate_asset_data.js",
    "migration:generate": "npm run typeorm:cli -- migration:generate -d src/db/cli.ts ",
    "migration:run": "docker-compose run --rm app ./node_modules/.bin/typeorm migration:run && npm run migrate:claim-subject",
    "migration:run:dev": "npx typeorm migration:run -d ormconfig.js && npm run migrate:claim-subject",
    "migration:revert": "docker-compose run --rm app ./node_modules/.bin/typeorm migration:revert",
    "migration:create": "docker-compose run --rm app ./node_modules/.bin/typeorm migration:create -n",
    "migration:test": "NODE_ENV=test ts-node ./node_modules/typeorm/cli.js migration:run -d src/db/cli.ts",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli.js",
    "release": "standard-version",
    "purge:extraneous-claims": "node dist/scripts/purge-extraneous-claims.js",
    "db:truncate": "ts-node -P tsconfig.json src/scripts/truncate-db.ts"
  },
  "dependencies": {
    "@ensdomains/ens-contracts": "^0.0.21",
    "@ensdomains/resolver": "^0.2.4",
    "@nestjs/axios": "2.0.0",
    "@nestjs/bull": "^0.6.0",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/graphql": "^11.0.4",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/terminus": "^10.0.1",
    "@nestjs/typeorm": "^10.0.0",
    "@sentry/integrations": "7.120.4",
    "@sentry/node": "7.120.4",
    "@sentry/tracing": "7.120.4",
    "@sphereon/ssi-types": "^0.36.0",
    "@spruceid/didkit-wasm-node": "^0.2.1",
    "@typechain/ethers-v5": "^9.0.0",
    "@types/jwt-decode": "^2.2.1",
    "@types/pako": "^2.0.0",
    "@types/passport": "^1.0.7",
    "bull": "^4.10.4",
    "chance": "^1.1.7",
    "class-transformer": "^0.5.0",
    "class-validator": "^0.14.0",
    "cockatiel": "^3.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "dgraph-js": "^21.3.1",
    "didkit-wasm-node": "^0.1.6",
    "dotenv": "^16.0.3",
    "eth-ens-namehash": "^2.0.8",
    "ethers": "5.7.2",
    "graphql": "^16.6.0",
    "graphql-fields": "^2.0.3",
    "graphql-tools": "^8.0.0",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "lodash.chunk": "^4.2.0",
    "ms": "^2.1.3",
    "multiformats": "^9.6.4",
    "nats": "^1.4.12",
    "pako": "^2.0.4",
    "parse-duration": "^1.0.2",
    "passport": "^0.7.0",
    "passport-did-auth": "2.1.0-dev.1346.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "redact-pii": "^3.2.3",
    "redis": "^4.6.4",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.3.0",
    "siwe": "^2.1.3",
    "swagger-ui-express": "^4.1.4",
    "typechain": "^8.1.1",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0",
    "wait-for-expect": "^3.0.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@energyweb/eslint-config": "^0.1.0",
    "@energyweb/prettier-config": "^0.0.1",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "@nomiclabs/hardhat-ethers": "^2.1.1",
    "@sentry/types": "7.120.4",
    "@types/bull": "^4.10.0",
    "@types/chance": "^1.1.3",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.3",
    "@types/jest": "^29.5.14",
    "@types/lodash.chunk": "^4.2.6",
    "@types/ms": "^0.7.31",
    "@types/node": "^13.9.1",
    "@types/passport-jwt": "^3.0.3",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "base64url": "^3.0.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "ethereum-waffle": "^4.0.10",
    "hardhat": "^2.11.2",
    "husky": "^4.3.8",
    "i": "^0.3.7",
    "ipfs": "^0.66.0",
    "ipfs-http-client": "^43.0.1",
    "ipfsd-ctl": "3.1.0",
    "jest": "^30.0.0",
    "jest-create-mock-instance": "^2.0.0",
    "lint-staged": "^11.1.1",
    "npm": "^8.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.1",
    "supertest": "^4.0.2",
    "ts-jest": "^29.3.4",
    "ts-loader": "^8.0.0",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.28.0",
    "typedoc-plugin-markdown": "^3.13.3",
    "typescript": "4.9.5",
    "wait-on": "^7.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build:docs && git add -A docs/api/"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
