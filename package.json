{
  "name": "iam-cache-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist && npm run build:contracts",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "npm run build:contracts && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest-e2e.json",
    "build:contracts": "npm run build:typechain:registry && npm run build:typechain:resolver && npm run build:typechain:didregistry",
    "build:typechain:didregistry": "typechain --target ethers-v4 --outDir src/ethers 'abi/EthereumDIDRegistry.json'",
    "build:typechain:registry": "typechain --target ethers-v4 --outDir src/ethers './node_modules/@ensdomains/ens/build/contracts/ENSRegistry.json'",
    "build:typechain:resolver": "typechain --target ethers-v4 --outDir src/ethers './node_modules/@ensdomains/resolver/build/contracts/PublicResolver.json'",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "docker:build": "docker-compose build app",
    "docker:start:app": "docker-compose up -d",
    "docker:start:resolver": "make start-universal-resolver",
    "docker:watch": "npm run docker:start:app && docker-compose logs -f app",
    "docker:down": "docker-compose down -v",
    "build:docs": "rimraf docs/api && typedoc --plugin typedoc-plugin-markdown --hideBreadcrumbs true",
    "generate:jwtkeys": "openssl genrsa -out private.pem 2048 && openssl rsa -in private.pem -pubout > public.pem",
    "migration:generate": "docker-compose run --rm app ./node_modules/.bin/typeorm migration:generate -n",
    "migration:run": "docker-compose run --rm app ./node_modules/.bin/typeorm migration:run",
    "migration:revert": "docker-compose run --rm app ./node_modules/.bin/typeorm migration:revert",
    "migration:create": "docker-compose run --rm app ./node_modules/.bin/typeorm migration:create -n"
  },
  "dependencies": {
    "@ensdomains/ens": "^0.4.5",
    "@ensdomains/resolver": "^0.2.4",
    "@ew-did-registry/did-document": "0.0.1-alpha.813.0",
    "@ew-did-registry/did-ethr-resolver": "0.0.1-alpha.827.0",
    "@ew-did-registry/did-ipfs-store": "0.0.1-alpha.632.0",
    "@nestjs/bull": "^0.2.2",
    "@nestjs/common": "^7.6.13",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.13",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.13",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/swagger": "^4.7.16",
    "@nestjs/typeorm": "^7.1.5",
    "@sentry/integrations": "^6.2.2",
    "@sentry/node": "^6.2.2",
    "@types/jwt-decode": "^2.2.1",
    "@types/node-fetch": "^2.5.7",
    "bull": "^3.18.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cockatiel": "^2.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "dgraph-js": "^20.3.0",
    "ethers": "^4.0.45",
    "grpc": "^1.24.3",
    "helmet": "^4.4.1",
    "jwt-decode": "^3.0.0",
    "lodash.chunk": "^4.2.0",
    "nats": "^1.4.12",
    "node-fetch": "^2.6.1",
    "passport-did-auth": "^1.0.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.4",
    "typechain": "^2.0.0",
    "typeorm": "^0.2.31",
    "useragent": "^2.3.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.6",
    "@nestjs/schematics": "^7.2.9",
    "@nestjs/testing": "^7.6.13",
    "@sentry/types": "^6.2.2",
    "@typechain/ethers-v4": "^1.0.1",
    "@types/bull": "^3.14.4",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.10",
    "@types/lodash.chunk": "^4.2.6",
    "@types/node": "^13.9.1",
    "@types/passport-jwt": "^3.0.3",
    "@types/redis": "^2.8.28",
    "@types/supertest": "^2.0.8",
    "@types/useragent": "^2.3.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.4.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.19.2",
    "typedoc-plugin-markdown": "^3.1.1",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
